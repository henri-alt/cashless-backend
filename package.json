  "main": "src/index.ts",
  "type": "module",
  "module": "ES2020",
  "scripts": {
    "start": "cross-env NODE_ENV=development tsx -r ./env/env-config.cjs src/index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --exec \"tsx -r ./env/env-config.cjs src/index.ts\"",
    "build": "cross-env NODE_ENV=production webpack --stats-error-details",
    "prod": "cross-env NODE_ENV=production tsx -r ./env/env-config.cjs dist/bundle.cjs",
    "test": "cross-env NODE_ENV=test jest --runInBand --silent=false --coverage --detectOpenHandles",
    "test-start": "cross-env NODE_ENV=test tsx -r ./env/env-config.cjs src/index.ts",
    "load-test": "cross-env NODE_ENV=test DEBUG=http artillery run --output \"./src/tests/load-tests/transactions-report.json\" ./src/tests/load-tests/transactions-load.yaml"
  },
